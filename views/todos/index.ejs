<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<body>
  <h2>Todos</h2>
  <ul>
    <% todos.forEach(function(t) { %>
      <li>
      <%= t.todo %>
       -
      <%= t.done ? 'done' : 'not done' %>
      </li>
    <% }); %>
  </ul>
</body>

</html>
